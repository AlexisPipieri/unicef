<div class="container">
  date: <%= @intervention.date_intervention %>

  <% @intervention.users.each do |plaideur|%>
    Plaideur: <%= plaideur.nom %>
  <% end %>
  <%= simple_form_for [@intervention, @plaideurintervention], :url => intervention_plaideurinterventions_path(@intervention), :method => :post do |f| %>
    <%= f.input :user_id %>
    <%= f.button :submit, class: 'btn-primary'%>
  <% end %>
</div>
